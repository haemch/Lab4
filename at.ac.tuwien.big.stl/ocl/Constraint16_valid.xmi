<?xml version="1.0" encoding="UTF-8"?>
<at.ac.tuwien.big.stl:System
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:at.ac.tuwien.big.stl="http://www.big.tuwien.ac.at/Stl"
    xsi:schemaLocation="http://www.big.tuwien.ac.at/Stl ../model/stl.ecore"
    name="TestSystem">
  <areas name="Area1">
    <components
        xsi:type="at.ac.tuwien.big.stl:Buffer"
        name="Buffer">
      <outputSlots
          name="bufferOutput"
          requiredType="//@itemTypes.0"/>
      <inputSlots
          name="bufferInput"
          requiredType="//@itemTypes.0"/>
    </components>
    <components
        xsi:type="at.ac.tuwien.big.stl:ItemGenerator"
        name="Generator"
        generatedType="//@itemTypes.0">
      <outputSlots
          name="generatorOut"
          requiredType="//@itemTypes.0"/>
    </components>
    <connectors
        entry="//@areas.0/@components.1/@outputSlots.0"
        exit="//@areas.0/@components.0/@inputSlots.0"/>
  </areas>
  <itemTypes
      name="T1"
      description="Typ"/>
</at.ac.tuwien.big.stl:System>
